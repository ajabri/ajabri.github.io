	    <script type="text/javascript">
	      function scroll()
	      {
	      var cur = Math.random()*100;
	      var next = parseInt(cur % 4, 10);
	      
	      //Keep in code - Written by Computerhope.com
	      //Place this script in your HTML heading section
	      //18-19 night
	      
	      if (next == 1)
	      {
              document.getElementById("hello").innerHTML = "Hello";
	      }
              else if (next == 2)
	      {
              document.getElementById('hello').innerHTML = "Salut";
	      }
	      else if (next == 3)
	      {
              document.getElementById('hello').innerHTML = "Nǐ Hǎo";
	      }
	      else if (next == 0)
	      {
              document.getElementById('hello').innerHTML = "Hola";
	      }
	      }  
              setInterval(function(){scroll()}, 1000);

	      $( document ).ready(function() {
	      $("#projects_link").on("click", function() {
	        
	      console.log("HELLO");
	      $(".grid").isotope({
	      itemSelector: '.grid-item',
	      layoutMode: 'fitRows'
	      });
	      $(".grid").show();
	      
	      });
	      });
	 

            </script>
